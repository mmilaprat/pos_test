{
    "name": "mmilaprat/test",
    "description": "Project template for deploy a Pos site (Drupal 8)  with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "Drupal8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "PoS-custom": {
            "type": "package",
                  "package": {
                    "name": "custom/pos",
                    "version": "master",
                    "type": "drupal-custom-module",                    
                    "source": {
                      "type": "git",
                      "url": "https://github.com/DRIVER-EU/PoS",
                      "reference": "master"
                    }
                  }
        },
        "PoS-theme": {            
            "type": "package",
                  "package": {
                    "name": "aleksip/shila-drupal-theme",
                    "version": "master",
                    "type": "drupal-custom-theme",                    
                    "source": {
                      "type": "git",
                      "url": "https://github.com/aleksip/shila-drupal-theme",
                      "reference": "master"
                    }
                  }
        }

    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/ajax_links_api": "^1.0",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/backup_migrate": "^4.0",
        "drupal/cancel_button": "^1.1",
        "drupal/colorbox": "^1.4",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/content_browser": "^1.0@alpha",
        "drupal/content_sync": "^2.0",
        "drupal/core": "^8.7.1",
        "drupal/ctools": "^3.0",
        "drupal/customerror": "^1.0",
        "drupal/devel": "^2.0",
        "drupal/diff": "^1.0",
        "drupal/disable_html5_validation": "^1.0",
        "drupal/drupalchat": "^1.0",
        "drupal/ds": "^3.3",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_clone": "^1.0@alpha",
        "drupal/entity_print": "^2.1",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/entity_reference_views_select": "^1.3",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/eu_cookie_compliance": "^1.5",
        "drupal/eva": "^1.3",
        "drupal/facets": "^1.3",
        "drupal/features": "^3.8",
        "drupal/field_group": "3.0-beta1",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/field_validation": "^1.0@alpha",
        "drupal/form_mode_manager": "^1.3",
        "drupal/gdoc_field": "^1.1",
        "drupal/group": "^1.0@RC",
        "drupal/h5p": "^1.0",
        "drupal/honeypot": "^1.29",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/link_attributes": "^1.5",
        "drupal/login_emailusername": "^1.1",
        "drupal/menu_block": "^1.5",
        "drupal/menu_export": "^1.2",
        "drupal/module_filter": "^3.1",
        "drupal/onomasticon": "^1.3",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/phpmailer": "^3.0@beta",
        "drupal/protected_file": "1.x-dev",
        "drupal/restui": "^1.16",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api": "^1.10",
        "drupal/search_api_attachments": " ^1.0@beta",
        "drupal/search_api_autocomplete": "^1.1",
        "drupal/search_api_page": "^1.0@alpha",
        "drupal/select_or_other": "^1.0@alpha",
        "drupal/simplify": "^1.0",
        "drupal/social_auth_linkedin": "^2.0",
        "drupal/starrating": "^4.0@alpha",
        "drupal/swagger_ui_formatter": "^2.1",
        "drupal/taxonomy_delete": "^1.0",
        "drupal/taxonomy_multidelete_terms": "^1.1",
        "drupal/taxonomy_term_depth": "^2.1",
        "drupal/twig_tweak": "^2.1",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/ultimate_cron": "^2.0",
        "drupal/video_embed_field": "^2.0",
        "drupal/view_mode_page": "^3.1",
        "drupal/views_base_url": "^1.0",
        "drupal/views_field_formatter": "^1.8",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/views_slideshow": "^4.6",
        "drupal/vote_up_down": "^1.0@alpha",
        "drupal/votingapi": "^3.0",
        "drupal/webform": "^5.0@RC",
        "drush/drush": "^9.0.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "mikehaertl/phpwkhtmltopdf": "^2.3",
        "tecnickcom/tcpdf": "~6",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "custom/pos": "*",
        "aleksip/shila-drupal-theme": "*"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
