    ],
    "repositories": {
        "Drupal8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "custom/pos_utils": {
            "type": "package",
                  "package": {
                    "name": "custom/pos_utils",
                    "version": "master",
                    "type": "drupal-custom-module",                    
                    "source": {
                      "type": "git",
                      "url": "https://github.com/mmilaprat/pos_utils",
                      "reference": "master"
                    }
                  }
        },
        "custom/reboot": {            
            "type": "package",
                  "package": {
                    "name": "custom/reboot",
                    "version": "master",
                    "type": "drupal-custom-theme",                    
                    "source": {
                      "type": "git",
                      "url": "https://github.com/mmilaprat/pos_theme",
                      "reference": "master"
                    }
                  }
        },
        "jquery-colorbox": {
            "type": "package",
                "package": {
                    "name": "jackmoore/colorbox",
                    "version": "1.6.4",
                    "type": "drupal-library",
                    "dist": {
                        "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
                        "type": "zip"
                    }
                }
        },
        "jquery.cycle": {
            "type": "package",
            "package": {
                "name": "jquery/cycle",
                "version": "3.0.3",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery.cycle"
                },
                "dist": {
                    "url": "https://github.com/malsup/cycle/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        "jquery.hoverIntent": {
            "type": "package",
            "package": {
                "name": "jmaerckaert/jquery.hoverIntent",
                "version": "1.9.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jmaerckaert/jquery.hoverIntent/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        "tobia/Pause": {
            "type": "package",
            "package": {
                "name": "tobia/Pause",
                "version": "0.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/tobia/Pause/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        "douglascrockford/JSON-js": {
            "type": "package",
            "package": {
                "name": "douglascrockford/JSON-js",
                "version": "0.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/douglascrockford/JSON-js/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        "swagger-ui": {
            "type": "package",
            "package": {
                "name": "swagger-api/swagger-ui",
                "version": "3.20.5",
                "type": "drupal-library",                    
                "source": {
                    "type": "git",
                    "url": "https://github.com/swagger-api/swagger-ui",
                    "reference": "master"
                }
            }
        },
        "phpwkhtmltopdf": {
            "type": "package",
            "package": {
                "name": "library_mikehaertl/phpwkhtmltopdf",
                "version": "2.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/mikehaertl/phpwkhtmltopdf/archive/master.zip",
                    "type": "zip"
                }
            }
        }        


    },
